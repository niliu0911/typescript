{
    "compilerOptions": {
        "jsx": "react",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "module": "commonjs",
        "noImplicitAny": true,
        "noEmitHelpers": true,
        "removeComments": true,
        "sourceMap": true,
        "target": "es5"
    },

    "exclude": [
        "node_modules",
        "tmp"
    ]
}
